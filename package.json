{
  "name": "one-to-one-chatg-app-backend",
  "version": "1.0.0",
  "description": "> A one to one chat application built using Node.js/Express/Socket.io and PostgreSQL/Sequelize ORM",
  "main": "index.js",
  "scripts": {
    "start": "node --require @babel/polyfill build/index.js",
    "dev-start": "nodemon --exec babel-node ./src/index.js",
    "db-migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "clean": "rm -rf build && mkdir build",
    "build-src": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-src"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "babel-loader": "^8.0.6",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "faker": "^4.1.0",
    "generate-password": "^1.5.1",
    "http-status": "^1.4.2",
    "http-status-codes": "^1.4.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.2",
    "moment": "^2.24.0",
    "nodemailer": "^6.4.13",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.4.1",
    "sequelize": "^5.22.4",
    "socket.io": "^4.2.0",
    "stripe": "^8.137.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "twilio": "^3.50.0",
    "uuid": "^3.4.0"
  },
